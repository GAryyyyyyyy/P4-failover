{
  "target": "bmv2",
  "p4info": "build/simple_recovery.p4.p4info.txt",
  "bmv2_json": "build/simple_recovery.json",
  "table_entries": [
    {
      "table": "MyIngress.ipv4_lpm",
      "match": {
        "hdr.ipv4.dstAddr": ["10.0.1.1", 32]
      },
      "action_name": "MyIngress.try_ipv4_forward",
      "action_params": {
        "port": 2
      }
    },
    {
      "table": "MyIngress.ipv4_lpm",
      "match": {
        "hdr.ipv4.dstAddr": ["10.0.2.2", 32]
      },
      "action_name": "MyIngress.try_ipv4_forward",
      "action_params": {
        "port": 1
      }
    },
    {
      "table": "MyIngress.port_backup_path",
      "match": {
        "standard_metadata.egress_spec": [2]
      },
      "action_name": "MyIngress.copy_path",
      "action_params": {
        "length": 3,
        "v1": 3,
        "v2": 2,
        "v3": 1,
        "v4": 0,
        "v5": 0,
        "v6": 0,
        "v7": 0,
        "v8": 0
      }
    },
    {
      "table": "MyIngress.edge_to_port",
      "match": {
        "meta.out_edge": [3]
      },
      "action_name": "MyIngress.recovery_forward",
      "action_params": {
        "port": 3
      }
    },
    {
      "table": "MyIngress.port_to_mac",
      "match": {
        "standard_metadata.egress_spec": [1]
      },
      "action_name": "MyIngress.update_mac_addr",
      "action_params": {
        "dstAddr": "08:00:00:00:02:22"
      }
    },
    {
      "table": "MyIngress.port_to_mac",
      "match": {
        "standard_metadata.egress_spec": [2]
      },
      "action_name": "MyIngress.update_mac_addr",
      "action_params": {
        "dstAddr": "08:00:00:00:01:00"
      }
    }
  ]
}
