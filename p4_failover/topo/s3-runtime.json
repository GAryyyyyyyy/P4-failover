{
  "target": "bmv2",
  "p4info": "build/simple_recovery.p4.p4info.txt",
  "bmv2_json": "build/simple_recovery.json",
  "table_entries": [
    {
      "table": "MyIngress.ipv4_lpm",
      "match": {
        "hdr.ipv4.dstAddr": ["10.0.1.1", 32]
      },
      "action_name": "MyIngress.try_ipv4_forward",
      "action_params": {
        "port": 2
      }
    },
    {
      "table": "MyIngress.ipv4_lpm",
      "match": {
        "hdr.ipv4.dstAddr": ["10.0.2.2", 32]
      },
      "action_name": "MyIngress.try_ipv4_forward",
      "action_params": {
        "port": 3
      }
    },
    {
      "table": "MyIngress.ipv4_lpm",
      "match": {
        "hdr.ipv4.dstAddr": ["10.0.3.3", 32]
      },
      "action_name": "MyIngress.try_ipv4_forward",
      "action_params": {
        "port": 1
      }
    },
    {
      "table": "MyIngress.port_to_mac",
      "match": {
        "standard_metadata.egress_spec": [2]
      },
      "action_name": "MyIngress.update_mac_addr",
      "action_params": {
        "dstAddr": "08:00:00:00:01:00"
      }
    },
    {
      "table": "MyIngress.port_to_mac",
      "match": {
        "standard_metadata.egress_spec": [3]
      },
      "action_name": "MyIngress.update_mac_addr",
      "action_params": {
        "dstAddr": "08:00:00:00:02:00"
      }
    },
    {
      "table": "MyIngress.port_to_mac",
      "match": {
        "standard_metadata.egress_spec": [1]
      },
      "action_name": "MyIngress.update_mac_addr",
      "action_params": {
        "dstAddr": "08:00:00:00:03:33"
      }
    }
  ]
}
